{
  "name": "gcc-make-run",
  "main": "./lib/gcc-make-run",
  "version": "0.1.0",
  "description": "Compile-run C/C++ source code and execute Makefile in Atom.",
  "keywords": [
    "gcc",
    "g++",
    "c++",
    "compile",
    "make",
    "run"
  ],
  "activationCommands": {
    "atom-workspace": [
      "gcc-make-run:compile-run",
      "gcc-make-run:run-options"
    ],
    ".tree-view .file > .name": [
      "gcc-make-run:make-run"
    ]
  },
  "repository": "https://github.com/tomlau10/gcc-make-run",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3"
  }
}
